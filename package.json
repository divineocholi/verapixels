{
  "name": "verapixels",
  "homepage": "https://verapixels.com",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && npm run copy-files",
    "copy-files": "node -e \"const fs = require('fs'); fs.copyFileSync('public/404.html', 'dist/404.html'); fs.copyFileSync('public/.nojekyll', 'dist/.nojekyll');\"",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist --dotfiles",
    "server": "nodemon server.js",
    "dev:all": "concurrently \"npm run server\" \"npm run dev\"",
    "start": "npm run dev:all"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@iconify/react": "^6.0.2",
    "@supabase/supabase-js": "^2.91.1",
    "@tailwindcss/vite": "^4.1.16",
    "appwrite": "^21.5.0",
    "bcryptjs": "^3.0.3",
    "cloudinary": "^2.8.0",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "framer-motion": "^12.23.24",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.548.0",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.13",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.5",
    "recharts": "^2.10.4",
    "resend": "^6.9.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "styled-jsx": "^5.1.7",
    "supabase": "^2.67.1",
    "swiper": "^12.0.3",
    "tailwind": "^2.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^24.10.4",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/react-helmet": "^6.1.11",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "gh-pages": "^6.3.0",
    "globals": "^16.4.0",
    "nodemon": "^3.1.11",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
